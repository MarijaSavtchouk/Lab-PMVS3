<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.bsu.mariacco.multiscreen">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Canari"
            android:label="@string/canari" />
        <activity android:name=".Curili" android:label="@string/curili" />
        <activity android:name=".Maldivi" android:label="@string/maldivi" />
        <activity android:name=".Philipini" android:label="@string/philipini"></activity>
    </application>

</manifest>